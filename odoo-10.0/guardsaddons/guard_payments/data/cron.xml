<odoo>
    <data>
        <record id="due_date_cron" model="ir.cron">
            <field name="name">Payment Due Days</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doal">1</field>
            <!--<field name="nextcall" >2016-12-31 23:59:59</field>-->
            <field name="model">guard.payments</field>
            <field name="function">_update_due_date</field>
            <field name="args" eval="" />
            <!--<field name="priority" eval="5" />-->
        </record>

        <record id="overdue_date_cron" model="ir.cron">
            <field name="name">Payment Overdue Days</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doal">1</field>
            <!--<field name="nextcall" >2016-12-31 23:59:59</field>-->
            <field name="model">guard.payments</field>
            <field name="function">_update_overdue_date</field>
            <field name="args" eval="" />
            <!--<field name="priority" eval="5" />-->
        </record>
    </data>
</odoo>